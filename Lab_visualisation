      "source": [
        "newx = np.linspace(0, 1, 500) # sample 500 X's between 0 and 1\n",
        "newy = np.sin(4 * np.pi * newx) * np.exp(-5 * newx) #formula for our graph \n",
        "\n",
        "fig, ax = plt.subplots()  #plt.subplots()  lets us acces the axis and plot seperately. \n",
        "\n",
        "ax.plot(newx, newy)"
