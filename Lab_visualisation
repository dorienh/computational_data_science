        "fig = plt.figure()\n",
        "ax = fig.add_subplot(1, 1, 1)\n",
        "\n",
        "line, = ax.plot(stockA)\n",
        "ax.set_yscale('log') #change the scale here\n",
        "\n",
        "plt.xlabel('Time')\n",
        "plt.ylabel('Stock A price')\n",
        "ax.set_title('Logarithmic')"
      ]
