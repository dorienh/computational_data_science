        "# notched plot\n",
        "plt.figure()\n",
        "plt.boxplot(x, 1)\n",
        "\n",
        "# change outlier point symbols\n",
        "plt.figure()\n",
        "plt.boxplot(x, 0, 'gD')\n",
        "\n",
        "# don't show outlier points\n",
        "plt.figure()\n",
        "plt.boxplot(x, 0, '')\n",
        "\n",
        "# horizontal boxes\n",
        "plt.figure()\n",
        "plt.boxplot(x, 0, 'rs', 0)\n",
        "\n",
